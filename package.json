{
    "version": "0.4.0",
    "keywords": [
        "react",
        "hooks",
        "utilities"
    ],
    "license": "Unlicense",
    "description": "Collection of useful React Hooks",
    "typings": "dist/index.d.ts",
    "source": "src/index.tsx",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "umd:main": "dist/index.umd.js",
    "unpkg": "dist/index.umd.js",
    "exports": {
        ".": "./dist/index.js",
        "./Keywords": "./dist/Keywords.js",
        "./Keywords.js": "./dist/Keywords.js"
    },
    "engines": {
        "node": ">=12.7.0"
    },
    "files": [
        "dist",
        "src"
    ],
    "peerDependencies": {
        "react": ">=17.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/vespaiach/react-utils"
    },
    "bugs": {
        "url": "https://github.com/vespaiach/react-utils/issues"
    },
    "name": "react-utils",
    "author": "Trinh Nguyen",
    "scripts": {
        "clean": "rimraf dist",
        "dev": "vite",
        "build": "npm run clean && microbundle --jsx React.createElement -i src/index.tsx -i src/Keywords.tsx --sourcemap false",
        "preview": "vite preview",
        "test": "vitest",
        "test:watch": "vitest run"
    },
    "devDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.30.5",
        "@typescript-eslint/parser": "^5.30.5",
        "@vitejs/plugin-react": "^1.3.2",
        "@vitest/ui": "^0.17.0",
        "eslint": "^8.19.0",
        "eslint-import-resolver-typescript": "^3.2.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-react": "^7.30.1",
        "happy-dom": "^6.0.2",
        "microbundle": "^0.15.0",
        "react-test-renderer": "^18.2.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.6.4",
        "vite": "^2.9.13",
        "vite-plugin-eslint": "^1.6.1",
        "vitest": "^0.17.0"
    }
}
